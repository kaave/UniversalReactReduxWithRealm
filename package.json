  "scripts": {
    "start": "run-s clean start:server server:development",
    "start:server": "NODE_ENV=development webpack --progress --config tools/webpack/server/index.ts",
    "build": "run-s clean build:*",
    "build:client": "NODE_ENV=production webpack --progress --config tools/webpack/client/index.ts",
    "build:server": "NODE_ENV=production webpack --progress --config tools/webpack/server/index.ts",
    "server": "NODE_ENV=production node ./build/server.js",
    "server:development": "NODE_ENV=development node --inspect ./build/server.js",
    "clean": "run-p clean:*",
    "clean:development": "rimraf data realm-object-server",
    "clean:production": "rimraf build",
    "lint": "run-p lint:*",
    "lint:scripts": "tslint 'source/**/*.{ts,tsx}'",
    "lint:scripts:fix": "tslint --fix 'source/**/*.{ts,tsx}' 'tools/**/*.ts'",
    "lint:styles": "stylelint 'source/**/*.css'",
